{
  "name": "changes.now.sh",
  "description": "See the changelog for any NPM package",
  "homepage": "https://documentup.com/Kikobeats/changes.now.sh",
  "version": "0.0.0",
  "main": "server.js",
  "author": {
    "name": "Kiko Beats",
    "email": "josefrancisco.verdu@gmail.com",
    "url": "https://kikobeats.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kikobeats/changes.now.sh.git"
  },
  "bugs": {
    "url": "https://github.com/Kikobeats/changes.now.sh/issues"
  },
  "keywords": [
    "changelog",
    "npm",
    "package"
  ],
  "dependencies": {
    "algoliasearch": "~3.30.0",
    "anchor-js": "~4.1.0",
    "autoprefixer": "~9.2.1",
    "babel-plugin-inline-dotenv": "~1.1.2",
    "babel-plugin-module-resolver": "~3.1.1",
    "babel-plugin-styled-components": "~1.8.0",
    "babel-plugin-wrap-in-js": "~1.1.1",
    "cssnano": "~4.1.5",
    "glob": "~7.1.2",
    "grid-styled": "~5.0.2",
    "hack": "~0.8.1",
    "isomorphic-unfetch": "~3.0.0",
    "next": "~7.0.2",
    "next-routes": "~1.4.1",
    "node-sass": "~4.9.4",
    "normalize.css": "~8.0.0",
    "postcss-easy-import": "~3.0.0",
    "postcss-focus": "~4.0.0",
    "postcss-loader": "~3.0.0",
    "raw-loader": "~0.5.1",
    "react": "~16.5.2",
    "react-dom": "~16.5.2",
    "react-feather": "~1.1.0",
    "react-load-script": "0.0.6",
    "react-markdown": "~4.0.3",
    "sass-loader": "~7.1.0",
    "styled-components": "~4.0.2"
  },
  "devDependencies": {
    "finepack": "latest",
    "git-dirty": "latest",
    "husky": "latest",
    "lint-staged": "latest",
    "prettier-standard": "latest",
    "standard": "latest",
    "standard-markdown": "latest"
  },
  "engines": {
    "node": ">= 8"
  },
  "scripts": {
    "build": "NODE_ENV=production next build",
    "clean": "rm -rf node_modules",
    "deploy": "now -e NODE_ENV=production --public && now alias && now rm changes --safe --yes",
    "dev": "NODE_ENV=development node server.js",
    "lint": "standard-markdown && standard",
    "precommit": "lint-staged",
    "pretest": "npm run lint",
    "pretty": "prettier-standard index.js {core,test,bin}/**/*.js --single-quote",
    "start": "NODE_ENV=production node server.js",
    "test": "exit 0"
  },
  "private": true,
  "license": "MIT",
  "lint-staged": {
    "package.json": [
      "finepack",
      "git add"
    ],
    "*.js": [
      "prettier-standard",
      "git add"
    ],
    "*.md": [
      "standard-markdown",
      "git add"
    ]
  }
}
